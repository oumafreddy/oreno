{
  "include": [
    "."
  ],
  "exclude": [
    "**/__pycache__",
    "**/node_modules",
    "**/.venv",
    "**/venv",
    "**/env",
    "**/staticfiles",
    "**/media"
  ],
  "executionEnvironments": [
    {
      "root": ".",
      "extraPaths": [
        "apps"
      ],
      "pythonVersion": "3.11"
    }
  ],
  "pythonVersion": "3.11",
  "typeCheckingMode": "basic",
  "reportMissingImports": true,
  "reportMissingTypeStubs": false
}

